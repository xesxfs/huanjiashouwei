         Locale.setString("TUTORIAL_BASICS_BUTTON_CANCEL","cn","跳过说明\n立刻开战！");
         Locale.setString("TUTORIAL_BASICS_BUTTON_NEXT","cn","进入教学\n模式");
         Locale.setString("TUTORIAL_TOWERS_BUTTON_CANCEL","cn","跳过说明\n立刻开战！");
